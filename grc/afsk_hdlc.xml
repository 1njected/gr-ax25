<block>
  <name>Hdlc</name>
  <key>afsk_hdlc</key>
  <category>AFSK</category>
  <import>import afsk</import>
  <make>afsk.hdlc($sample_rate, $subsample, $debug_level)</make>
  <param>
    <name>Sample_rate</name>
    <key>sample_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Subsample</name>
    <key>subsample</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>Debug_level</name>
    <key>debug_level</key>
    <value>5</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
   <name>out</name>
   <type>byte</type>
  </source>
</block>
